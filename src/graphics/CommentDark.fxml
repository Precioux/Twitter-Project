        </Label>
        <TextField fx:id="commentBox" layoutX="83.0" layoutY="119.0" prefHeight="161.0" prefWidth="433.0" />
        <Button fx:id="back" layoutX="161.0" layoutY="305.0" onAction="#backTimeLine" prefHeight="44.0" prefWidth="94.0" style="-fx-background-color: #940000;" text="_Back" textFill="WHITE">
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
        </Button>
        <Button fx:id="comment" layoutX="300.0" layoutY="305.0" onAction="#commentIt" prefHeight="44.0" prefWidth="94.0" style="-fx-background-color: #940000;" text="_comment" textFill="WHITE">
