<AnchorPane fx:id="mainArea" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #616161;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="graphics.commentControllerDarkImp">
    <children>
        <Label layoutX="55.0" layoutY="77.0" text="Enter you comment :" textFill="WHITE">
            <font>
                <Font name="System Bold" size="19.0" />
